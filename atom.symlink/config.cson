"*":
  "atom-beautify":
    general:
      _analyticsUserId: "798dbaa8-dc67-4fa3-aa15-738312108e7e"
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "jslint"
      "linter-eslint"
      "math-selection"
      "linter"
      "linter-coffeelint"
    ]
    hideGitIgnoredFiles: true
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    themes: [
      "atom-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    backUpBeforeSaving: true
    fontFamily: "Menlo"
    invisibles: {}
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    softWrapAtPreferredLineLength: false
  emmet: {}
  "exception-reporting":
    userId: "da12a123-0b08-19c5-f3af-964885fa16c4"
  "file-icons": {}
  "git-log": {}
  linter:
    showErrorPanel: false
  "linter-eslint": {}
  "markdown-preview": {}
  minimap:
    absoluteMode: true
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  pigments: {}
  react: {}
  "release-notes":
    viewedVersion: "0.89.0"
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  "status-bar": {}
  tabs:
    usePreviewTabs: true
  "todo-show":
    showInTable: [
      "Text"
      "Type"
      "File"
      "Line"
    ]
  "tree-view": {}
  welcome:
    showOnStartup: false
".basic.html.text":
  editor:
    preferredLineLength: 100
".coffee.jsx.source":
  editor:
    preferredLineLength: 100
".coffee.source":
  editor:
    preferredLineLength: 100
".diff.source":
  editor:
    preferredLineLength: 100
".html.spacebars.text":
  editor:
    preferredLineLength: 100
".ini.source":
  editor:
    preferredLineLength: 100
".js.jsx.source":
  editor:
    preferredLineLength: 100
".js.source":
  editor:
    preferredLineLength: 100
